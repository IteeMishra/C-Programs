#include<bits/stdc++.h>
using namespace std;
int main(){
int n; 
cout<<"Input size of array"<<endl;
cin>>n;
if(n==0)
{
    cout<<"the array is empty";
    return 0;
}
cout<<"input the array elements"<<endl;
int a[n];
for (int i=0;i<n;i++)
{
    cin>>a[i];
}
n=sizeof(a)/sizeof(a[0]);
sort(a,a+n);
cout<<"Maximum Element is "<<a[n-1]<<endl;
cout<<"Minimum Element is"<<a[0]<<endl;

return 0;

}
